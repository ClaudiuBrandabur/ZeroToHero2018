<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">
    
    <changeSet id="change-1.0" author="andreeag">
        <createTable tableName="PEOPLE">
            <column name="PERS_ID" type="NUMBER(6)">
                <constraints primaryKey="true" />
            </column>
            <column name="FIRST_NAME" type="VARCHAR2(40BYTE)"></column>
            <column name="LAST_NAME" type="VARCHAR2(40BYTE)"></column>
            <column name="EMAIL" type="VARCHAR2(30BYTE)">
                <constraints nullable="false"/>
            </column>
            <column name="PHONE_NUMBER" type="VARCHAR2(25BYTE)"></column>
            <column name="ADDRESS_ID" type="NUMBER(6)">
                <constraints nullable="false"/>
            </column>

        </createTable>
    </changeSet>

</databaseChangeLog>
